    "core-js": "^3.9.1",
    "nuxt": "^2.15.3",
    "prism-themes": "^1.6.0",
    "rehype-mathjax": "^3.1.0",
    "remark-emoji": "^2.2.0",
    "remark-math": "^4.0.0",
    "remark-oembed": "^1.2.2",
    "twemoji": "^13.0.2",
    "vue-twemoji": "^1.0.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.0.1",
    "@commitlint/config-conventional": "^12.0.1",
